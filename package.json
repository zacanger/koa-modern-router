{
  "name": "koa-modern-router",
  "description": "Router middleware for koa. Provides RESTful resource routing.",
  "version": "8.2.4",
  "author": "Alex Mingoia <talk@alexmingoia.com>",
  "license": "MIT",
  "main": "lib/router.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "_test": "npm run test:lint && test:tape",
    "cover": "nyc npm run test:tape",
    "doc": "NODE_ENV=test jsdoc2md -t ./lib/README_tpl.hbs --src ./lib/*.js  >| README.md",
    "preversion": "npm run doc && sortpack && npm t",
    "test": "npm run cover",
    "test:lint": "eslint -c .eslintrc.json lib/**/*.js test/**/*.js",
    "test:tape": "NODE_ENV=test mocha test/**/*.js"
  },
  "homepage": "https://github.com/zacanger/koa-modern-router#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/zacanger/koa-modern-router.git"
  },
  "bugs": "https://github.com/zacanger/koa-modern-router/issues",
  "keywords": [
    "koa",
    "koa-router",
    "middleware",
    "route",
    "router"
  ],
  "dependencies": {
    "http-errors": "1.7.1",
    "jsdoc-to-markdown": "1.1.1",
    "koa-compose": "4.1.0",
    "methods": "1.1.2",
    "path-to-regexp": "1.1.1",
    "urijs": "1.19.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.1",
    "eslint": "5.13.0",
    "eslint-config-zacanger": "3.4.2",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-import": "2.16.0",
    "eslint-plugin-node": "8.0.1",
    "eslint-plugin-promise": "4.0.1",
    "eslint-plugin-unicorn": "7.1.0",
    "expect.js": "0.3.1",
    "koa": "2.7.0",
    "mocha": "2.0.1",
    "nyc": "13.2.0",
    "should": "6.0.3",
    "sortpack": "1.1.2",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "nyc": {
    "include": [
      "lib/*.js"
    ],
    "instrument": true,
    "reporter": [
      "text",
      "html",
      "json"
    ],
    "sourceMap": true
  }
}
